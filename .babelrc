{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "node": "7.6.0"
                }
            }
        ]
    ],
    "plugins": [
        [
            "@babel/plugin-proposal-decorators",
            {
                "decoratorsBeforeExport": false
            }
        ],
        "@babel/plugin-transform-runtime"
    ]
}
